export int gcd(int a, int b) {
    int solution;
    int biggest;
    if (a < 0) {
        a = a * -1;
    }
    if (b < 0) {
        b = b * -1;
    }
    if (a > b) {
        biggest = a;
    }
    else {
        biggest = b;
    }
    for (int i = 1, biggest) {
        if (a % i == 0 && b % i == 0) {
            solution = i;
        }
    }
    return solution;
}

export int fac(int n) {
    int factorial = 1;
    if (n < 0) {
        return -1;
    }
    for (int i = 1, n + 1) {
        factorial = factorial * i;
    }
    return factorial;
}

export int fib(int n) {
    if (n == 1) {
        return 0;
    }
    if (n <= 2) {
        return 1;
    }
    return fib(n - 1) + fib(n - 2);
}

export bool isprime(int n) {

    if (n < 2) {
        return false;
    }
    for (int i = 2, n) {
        if (n % i == 0) {
            return false;
        }
    }
    return true;
}

